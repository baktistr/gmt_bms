{
    "name": "bms/nova-theme-css",
    "description": "A Laravel Nova theme.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Bms\\NovaThemeCss\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bms\\NovaThemeCss\\ThemeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
